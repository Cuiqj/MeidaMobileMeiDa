<?xml version="1.0" encoding="UTF-8"?>
<pdf:template>
    {{! 公路赔（补）偿通知书 }}
    <pdf:paper width="595.276" height="841.89" fontName="SimSun" fontSize="13" offset="-2,-20" />
    <pdf:content>
        <pdf:text point="75,50" fontName="SimHei" fontSize="17">公路赔（补）偿通知书</pdf:text>
        
        <pdf:text point="65,62.5">（</pdf:text>
        <pdf:line fromPoint="69.5,66.5" toPoint="83,66.5" lineWidth="0.5"/>
        <pdf:text point="71,62.5" static="NO">{{ caseMark2 }}</pdf:text>
        <pdf:text point="83.3,62.3">）年</pdf:text>
        <pdf:line fromPoint="95,66.5" toPoint="110,66.5" lineWidth="0.5"/>
        <pdf:text point="95,62.2" static="NO">{{ casePrefix }}</pdf:text>
        <pdf:text point="110,62.5">交赔字第</pdf:text>
        <pdf:line fromPoint="130,66.5" toPoint="142,66.5" lineWidth="0.5"/>
        <pdf:text point="130,62.2" maxWidth="12" static="NO">{{ caseMark3 }}</pdf:text>
        <pdf:text point="142,62.5">号</pdf:text>
        <pdf:line fromPoint="21,80.5" toPoint="182,80.5" lineWidth="0.8"/>
        <pdf:text point="23,91.5">当  事  人:</pdf:text>
        <pdf:line fromPoint="54,96" toPoint="95,96" lineWidth="0.5"/>
        <pdf:text point="55,92" static="NO" fontSize="10">{{ partyName }}</pdf:text>
        <pdf:text point="23,103">当事人地址:</pdf:text>
        <pdf:line fromPoint="54,108" toPoint="120,108" lineWidth="0.5"/>
        <pdf:text point="55,104" maxWidth="90" static="NO" fontSize="10">{{ partyAddress }}</pdf:text>
        
        <pdf:text point="34,114.5">当事人</pdf:text>
        <pdf:line fromPoint="48.5,119" toPoint="95,119" lineWidth="0.5"/>
        <pdf:paragraph rect="{48.5,112.5},{46.5,9}" indent="head:0" lineHeight="3.5" static="NO" fontSize="10">{{ caseReason }}</pdf:text>
        <pdf:text point="98,114.5">一案，经</pdf:text>
        <pdf:line fromPoint="118,119" toPoint="151,119" lineWidth="0.5"/>
        <pdf:text point="117.5,113" maxWidth="33.5" static="NO" fontSize="10">{{ agencyCity }}</pdf:text>
        <pdf:text point="117.5,116.5" maxWidth="33.5" static="NO" fontSize="10">{{ agency }}</pdf:text>
        <pdf:text point="152,114.5">依法调查核实:</pdf:text>
        <pdf:text point="34,125">当事人</pdf:text>
        <pdf:line fromPoint="48.5,130" toPoint="185,130" lineWidth="0.5"/>
        <pdf:line fromPoint="23,140.5" toPoint="187,140.5" lineWidth="0.5"/>
        <pdf:line fromPoint="23,151" toPoint="162,151" lineWidth="0.5"/>
        <pdf:paragraph rect="{23,122.5},{164,32}" indent="head:26.5" lineHeight="10" static="NO" fontSize="10">{{ caseDescription }}</pdf:paragraph>
        <pdf:text point="34,156.5">上述事实，由</pdf:text>
        <pdf:line fromPoint="64.5,160.5" toPoint="182,160.5" lineWidth="0.5"/>
        <pdf:text point="182.5,156.5">予</pdf:text>
        <pdf:text point="70,156.5" static="NO" fontSize="10">{{ caseEvidence }}</pdf:text>
        <pdf:text point="23,167">以佐证。本单位认为，当事人损坏路产的事实清楚，应依法承担民事责任，</pdf:text>
        <pdf:text point="23,178">赔偿路产损失。依</pdf:text>
        <pdf:line fromPoint="63,182.5" toPoint="187,182.5" lineWidth="0.8"/>
        <pdf:line fromPoint="23,193" toPoint="182,193" lineWidth="0.8"/>
        <pdf:text point="182.5,189">之</pdf:text>
        <pdf:paragraph rect="{23,177.5},{165,18}" indent="head:41.5" lineHeight="5"  static="NO" fontSize="9">{{ payReason1 }}</pdf:paragraph>
<!--        <pdf:paragraph rect="{23,171},{165,16}" lineHeight="8"  static="NO" fontSize="9">{{ payReason2 }}</pdf:paragraph>-->
        <pdf:text point="23,198.5">规定，本单位依法做出如下公路赔（补）偿处理认定:</pdf:text>
        <pdf:text point="34,209.5">当事人赔（补）偿</pdf:text>
        <pdf:text point="124,209.5">元。当事人必须在收到本通知</pdf:text>
        <pdf:text point="23,220.5">书之日起15日内持本通知书到</pdf:text>
        <pdf:text point="133.5,220.5">缴纳路产赔（补）偿费。</pdf:text>
        <pdf:line fromPoint="76.5,214.5" toPoint="123.5,214.5" lineWidth="0.5"/>
        <!--   rect="{74,204},{50,25}   frame   {x,y}{width,height}   -->
        <pdf:paragraph rect="{76,205.5},{49,25}" indent="head:0" lineHeight="4" static="NO" fontSize="10">{{ payDetail }}</pdf:paragraph>
        <pdf:line fromPoint="94,225.5" toPoint="133,225.5" lineWidth="0.5"/>
        <pdf:paragraph rect="{95,218},{37,55}" indent="head:0" lineHeight="4"  static="NO" fontSize="10">{{ paymentPlace }}</pdf:paragraph>
        <pdf:text point="75,217"></pdf:text>
    
        <pdf:text point="34,231">当事人对本通知书认定的事实和赔（补）偿费数额有疑义的，可在收</pdf:text>
        <pdf:text point="25,242">到本通知书之日起60日内向</pdf:text>
        <pdf:line fromPoint="90,247" toPoint="126,247" lineWidth="0.8"/>
        <pdf:text point="90.5,242" maxWidth = "35" static="NO" fontSize="10" >{{ reviewOrgan }}</pdf:text>
        <pdf:text point="126,242">申请复核，或者依照《民事诉</pdf:text>
        <pdf:text point="25,253">讼法》的规定向人民法院提起民事起诉。</pdf:text>
        <!--    <pdf:text point="108,270">公路赔（补）偿认定单位（印章）</pdf:text>    -->
        <pdf:text point="143,279">年     月     日</pdf:text>
        <pdf:text point="131,280" static="NO" fontSize="10">{{year}}</pdf:text>
        <pdf:text point="152,280" static="NO" fontSize="10">{{month}}</pdf:text>
        <pdf:text point="167,280" static="NO" fontSize="10">{{day}}</pdf:text>
    </pdf:content>
</pdf:template>
